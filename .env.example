# Environment Variables for ClassSync
# Copy this file to .env.local and fill in the values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your_project-default-rtdb.firebasedatabase.app
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Apps Script Backend URL (optional - for GAS backend integration)
VITE_GAS_BACKEND_URL=https://script.google.com/macros/s/your_deployment_id/exec

# Gemini API Key (optional - for AI features)
VITE_GEMINI_API_KEY=your_gemini_api_key
